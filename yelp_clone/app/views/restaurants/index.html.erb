<% if @restaurants.any? %>
  <% @restaurants.each do |restaurant| %>
    <h2><%= link_to restaurant.name, restaurant_path(restaurant) %></h2>
    <!-- <h2><%= restaurant.name %></h2> REMOVE THIS LINE -->
    <h3>Rating: <%= restaurant.rating %></h3>
    <% end %>
<% else %>
  <h1>No restaurants have been listed yet, click below to add a new resturant.</h1>
<% end %>

<a href="#">Add a restaurant</a>
